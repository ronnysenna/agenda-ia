# Example file for production environment
# Rename to .env.production and fill in the values

# Database
DATABASE_URL="postgresql://user:password@postgres:5432/agendaai?schema=public"

# Next.js settings
NEXTAUTH_URL="https://vendamais-front.dgohio.easypanel.host"
NEXTAUTH_SECRET="use-a-secure-random-string"
NEXT_PUBLIC_BUILD_ID="build-id"

# Email settings
EMAIL_SERVER_HOST="smtp.example.com"
EMAIL_SERVER_PORT="587"
EMAIL_SERVER_USER="user@example.com"
EMAIL_SERVER_PASSWORD="smtp-password"
EMAIL_FROM="noreply@yourdomain.com"

# Cloudinary settings
CLOUDINARY_CLOUD_NAME="your-cloud-name"
CLOUDINARY_API_KEY="your-api-key"
CLOUDINARY_API_SECRET="your-api-secret"

# Cache and performance
NODE_ENV=production

# Trusted origins (comma separated)
TRUSTED_ORIGINS="https://vendamais-front.dgohio.easypanel.host,http://localhost:3000"
