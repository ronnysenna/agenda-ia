{
  "name": "agenda-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "npx prisma generate && NODE_ENV=production next build",
    "build:clean": "rm -rf .next && npx prisma generate && NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "start:with-env": "NODE_ENV=production dotenv -e .env -- next start",
    "lint": "next lint",
    "generate": "prisma generate",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:layout": "vitest run src/tests/layout-cache.test.ts",
    "cache:clear": "rm -rf .next/cache"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.0.1",
    "@phosphor-icons/react": "^2.1.7",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tabler/icons-react": "^3.35.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/next-auth": "^3.13.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react-big-calendar": "^1.16.2",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.2.7",
    "bootstrap": "^5.3.8",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.6.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.18.1",
    "lucide-react": "^0.508.0",
    "next": "14.2.6",
    "next-auth": "^4.24.5",
    "nodemailer": "^6.9.9",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.5.5",
    "msw": "^2.10.2",
    "playwright-chromium": "^1.53.0",
    "prettier": "^3.2.5",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5",
    "vitest": "^1.0.0"
  },
  "nixpacks": {
    "phases": {
      "setup": {
        "nixPkgs": [
          "nodejs_20",
          "yarn"
        ]
      }
    }
  },
  "engines": {
    "node": ">=20.11.0"
  }
