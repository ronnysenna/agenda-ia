# Arquivo .env.example para configuração do AgendaAI no Easypanel

# Banco de dados
DATABASE_URL="postgresql://user:password@postgres:5432/agendaai?schema=public"

# Configurações do Next.js
NEXTAUTH_URL="https://seu-dominio.com"
NEXTAUTH_SECRET="sua-chave-secreta-para-nextauth"

# Configurações de e-mail (opcional, dependendo das funcionalidades do app)
EMAIL_SERVER_HOST="smtp.example.com"
EMAIL_SERVER_PORT="587"
EMAIL_SERVER_USER="user@example.com"
EMAIL_SERVER_PASSWORD="senha-do-smtp"
EMAIL_FROM="noreply@seu-dominio.com"

# Configurações Cloudinary (se estiver usando)
CLOUDINARY_CLOUD_NAME="seu-cloud-name"
CLOUDINARY_API_KEY="sua-api-key"
CLOUDINARY_API_SECRET="seu-api-secret"
